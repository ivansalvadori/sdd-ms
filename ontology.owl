@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix api:   <http://www.public-security-ontology/> .

api:xavinha  a  owl:ObjectProperty .

api:hasLocation  a              owl:ObjectProperty ;
        owl:equivalentProperty  api:xavinha .

api:ReportCarroRoubado
        a                    owl:Class ;
        owl:equivalentClass  [ a                   owl:Class ;
                               owl:intersectionOf  ( <http://www.public-security-ontologyTheftAutoReport>
                                                     [ a               owl:Restriction ;
                                                       owl:hasValue    "AUTOMOVEL" ;
                                                       owl:onProperty  api:typeOfVehicule
                                                     ]
                                                     [ a               owl:Restriction ;
                                                       owl:hasValue    "MOTO" ;
                                                       owl:onProperty  api:licencePlate
                                                     ]
                                                   )
                             ] .

api:neime  a    owl:DatatypeProperty .

api:reportNumber  a  owl:DatatypeProperty .

api:TheftAutoReport  a  owl:Class .

api:Address  a  owl:Class .

api:StolenVehicule  a  owl:Class .

api:cityName  a                 owl:DatatypeProperty ;
        owl:equivalentProperty  api:neime .

api:City  a     owl:Class .

api:CriminalReport  a        owl:Class ;
        owl:equivalentClass  api:TheftAutoReport .
