@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix api:   <http://www.public-security-ontology/> .

api:CaminhaoFurtado  a       owl:Class ;
        owl:equivalentClass  [ a                   owl:Class ;
                               owl:intersectionOf  ( api:VeiculoFurtado
                                                     [ a               owl:Restriction ;
                                                       owl:hasValue    "CAMINH√ÉO" ;
                                                       owl:onProperty  api:tipoVeiculo
                                                     ]
                                                   )
                             ] .

api:MotoFurtada  a           owl:Class ;
        owl:equivalentClass  [ a                   owl:Class ;
                               owl:intersectionOf  ( api:VeiculoFurtado
                                                     [ a               owl:Restriction ;
                                                       owl:hasValue    "MOTOCICLO" ;
                                                       owl:onProperty  api:tipoVeiculo
                                                     ]
                                                   )
                             ] .

api:localDaOcorrencia
        a              owl:ObjectProperty ;
        owl:inverseOf  api:possuiBoletimOcorrencia .

api:numeroOcorrencia  a  owl:DatatypeProperty .

api:veiculoFurtado  a  owl:ObjectProperty ;
        owl:inverseOf  api:possuiBoletimOcorrencia .

api:BOnoturno  a             owl:Class ;
        owl:equivalentClass  [ a                   owl:Class ;
                               owl:intersectionOf  ( api:BO
                                                     [ a               owl:Restriction ;
                                                       owl:hasValue    "A NOITE" ;
                                                       owl:onProperty  api:periodoOcorrencia
                                                     ]
                                                   )
                             ] .

api:BO  a                    owl:Class ;
        owl:equivalentClass  api:BoletimOcorrenciaFurtoVeiculo .

api:GeoLocalizacao  a  owl:Class .

api:tipoVeiculo  a  owl:DatatypeProperty .

api:VeiculoFurtado  a  owl:Class .

api:LocalDaOcorrencia
        a       owl:Class .

api:possuiBoletimOcorrencia
        a       owl:ObjectProperty .

api:placa  a    owl:DatatypeProperty .

api:periodoOcorrencia
        a       owl:DatatypeProperty .

api:BoletimOcorrenciaFurtoVeiculo
        a       owl:Class .
